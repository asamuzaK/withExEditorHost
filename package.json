{
  "name": "withexeditorhost",
  "description": "Native messaging host for withExEditor",
  "author": "asamuzaK",
  "homepage": "https://github.com/asamuzaK/withExEditorHost",
  "bugs": "https://github.com/asamuzaK/withExEditorHost/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/asamuzaK/withExEditorHost.git"
  },
  "license": "MIT",
  "main": "./index.js",
  "type": "module",
  "scripts": {
    "bundle:js": "esbuild ./index.js --bundle --platform=node --outdir=./bundle --minify",
    "bundle:bin": "boxednode -n lts -s ./bundle/index.js",
    "genversion": "genversion --es6 --semi ./modules/version.js",
    "lint": "eslint --fix .",
    "prebuild": "npm run genversion && npm run lint && npm run test",
    "setup": "node ./index.js setup",
    "start": "node ./index.js",
    "test": "c8 --reporter=text mocha test/*.test.js --exit",
    "zip:jszip": "jszip"
  },
  "dependencies": {
    "commander": "^8.2.0",
    "https-proxy-agent": "^5.0.0",
    "node-fetch": "^3.0.0",
    "readline-sync": "^1.4.10",
    "semver-parser": "^4.0.0",
    "web-ext-native-msg": "^5.0.0"
  },
  "devDependencies": {
    "boxednode": "^1.10.2",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "esbuild": "^0.13.7",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "genversion": "^3.0.1",
    "jszip-cli": "^1.4.24",
    "mocha": "^9.1.3",
    "nock": "^13.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2"
  },
  "version": "7.0.0"
}
