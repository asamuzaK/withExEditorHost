{
  "name": "withexeditorhost",
  "description": "Native messaging host for withExEditor",
  "author": "asamuzaK",
  "homepage": "https://github.com/asamuzaK/withExEditorHost",
  "bugs": "https://github.com/asamuzaK/withExEditorHost/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/asamuzaK/withExEditorHost.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "./index.js",
  "scripts": {
    "build": "npm run clean && npm-run-all -p build:*",
    "build:linx": "pkg ./index.js --targets linux --out-path ./dest/linux",
    "build:macos": "pkg ./index.js --targets macos --out-path ./dest/macos",
    "build:win32": "pkg ./index.js --targets win-x86 --out-path ./dest/win32",
    "build:win64": "pkg ./index.js --targets win-x64 --out-path ./dest/win64",
    "clean": "rimraf ./dest/*",
    "lint": "eslint .",
    "setup": "npm install && node ./index.js --setup",
    "start": "node ./index.js",
    "test": "mocha test"
  },
  "dependencies": {
    "semver-parser": "^1.0.15",
    "web-ext-native-msg": "^2.2.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.18.0",
    "mocha": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "pkg": "^4.3.0",
    "rimraf": "^2.6.2"
  },
  "version": "2.2.6"
}
