{
  "name": "withexeditorhost",
  "description": "Native messaging host for withExEditor",
  "author": "asamuzaK",
  "homepage": "https://github.com/asamuzaK/withExEditorHost",
  "bugs": "https://github.com/asamuzaK/withExEditorHost/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/asamuzaK/withExEditorHost.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "./index.js",
  "scripts": {
    "bundle": "ncc build ./index.js -m -o bundle",
    "lint": "eslint --fix .",
    "package": "boxednode -n lts -s ./bundle/index.js",
    "setup": "npm install --production && node ./index.js setup",
    "start": "node ./index.js",
    "test": "nyc --reporter=text mocha test/*.test.js --exit",
    "zippkg:linux": "jszip ./LICENSE ./README.md ./README.ja.md ./pkg/linux/index -o ./dist/linux-x86_64.zip",
    "zippkg:macos": "jszip ./LICENSE ./README.md ./README.ja.md ./pkg/macos/index -o ./dist/macos.zip",
    "zippkg:win": "jszip ./LICENSE ./README.md ./README.ja.md ./pkg/win/index.exe -o ./dist/win64.zip"
  },
  "dependencies": {
    "commander": "^6.2.0",
    "global-agent": "^2.1.12",
    "package-json": "^6.5.0",
    "readline-sync": "^1.4.10",
    "semver-parser": "^3.1.0",
    "web-ext-native-msg": "^4.8.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.25.1",
    "boxednode": "^1.7.1",
    "chai": "^4.2.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jszip-cli": "^1.4.24",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rewiremock": "^3.14.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.0"
  },
  "version": "6.0.0-a.16"
}
