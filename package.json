  "license": "MIT",
  "homepage": "https://github.com/asamuzaK/withExEditorHost",
  "bugs": "https://github.com/asamuzaK/withExEditorHost/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/asamuzaK/withExEditorHost.git"
  },
  "type": "module",
  "main": "./index.js",
  "dependencies": {
    "commander": "^10.0.0",
    "https-proxy-agent": "^5.0.1",
    "node-fetch": "^3.3.1",
    "readline-sync": "^1.4.10",
    "semver-parser": "^4.1.3",
    "web-ext-native-msg": "^6.1.7"
  },
  "devDependencies": {
    "boxednode": "^2.0.1",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.3",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.13.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "genversion": "^3.1.1",
    "jszip-cli": "^1.4.24",
    "mocha": "^10.2.0",
    "nock": "^13.3.0",
    "sinon": "^15.0.2"
  },
  "scripts": {
    "bundle-js": "esbuild --platform=node --outdir=./bundle --bundle --minify ./index.js",
    "bundle-bin": "boxednode --node-version=lts --source=./bundle/index.js",
    "genversion": "genversion --es6 --semi ./modules/version.js",
    "lint": "eslint --fix .",
    "prebuild": "npm run genversion && npm run lint && npm run test",
    "setup": "node ./index.js setup",
    "start": "node ./index.js",
    "test": "c8 --reporter=text mocha --exit test/*.test.js",
    "zip-jszip": "jszip"
  },
  "version": "7.1.2"
}
